class Node:
    def __init__(self, data) -> None:
        self.data = data
        self.next = None

class linkedList:
    def __init__(self) -> None:
        self.head = None
    
    def print(self):
        temp = self.head
        while temp:
            print(temp)
            temp = temp.next

if __name__ == "__main__":
    llist = linkedList()
    llist.head = Node(1)
    second = Node(2)
    third = Node(3)

    llist.head.next = second
    second.next = third
